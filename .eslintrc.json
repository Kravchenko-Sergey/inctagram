{
  "extends": ["next/core-web-vitals", "@it-incubator/eslint-config"],
  "overrides": [
    {
      "files": ["*.stories.tsx"],
      "rules": {
        "react-hooks/rules-of-hooks": "off"
      }
    }
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "import/order": "off",
    "react/no-unknown-property": [
      2,
      {
        "ignore": ["jsx", "global"]
      }
    ]
  }
}
